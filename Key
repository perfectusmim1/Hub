local Games = {
    [89438510123061] = "https://raw.githubusercontent.com/perfectusmim1/Games/refs/heads/main/animeshadow"
}
if Games[game.PlaceId] then
    loadstring(game:HttpGet(Games[game.PlaceId]))()
else
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "PERFECTUS HUB", -- Required
	Text = "Game not supported", -- Required
	Icon = "rbxassetid://17009514249" -- Optional
})
end
